{
  "rules": {
    "users": {
      ".read": "auth != null",
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    },
    "game": {
      ".read": "auth != null",
      ".write": false
    },
    "gameInfo": {
      ".read": "auth != null",
      ".write": false
    },
    "gameChat": {
      ".read": "auth != null",
      ".write": false
    },
    "req": {
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    }
  }
}
